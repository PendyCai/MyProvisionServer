;
; BIND data file for local loopback interface
;
$ORIGIN sdtpktc.com.
$TTL	604800
@	IN	SOA	sdtpktc.com. root.sdtpktc.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	sdtpktc.com.
@	IN	A	192.168.10.220
@	IN	AAAA	2001:da8:8000:d010:0:5efe:192.168.10.220

ns	IN	A	192.168.10.220
ns	IN	AAAA	2001:da8:8000:d010:0:5efe:192.168.10.220
provserv	IN	CNAME	ns
cmts    IN      A       192.168.10.254


